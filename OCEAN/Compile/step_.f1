      if (synchro_flag) then
            call get_bry
        call get_vbc
        synchro_flag=.false.
      endif  !<-- synchro_flag
C$OMP END MASTER
C$OMP BARRIER


      if (may_day_flag.ne.0) return  !-->  EXIT


