        call rho_eos (tile)
        call set_HUV (tile)
        call diag(tile)


      enddo
C$OMP BARRIER
      do tile=my_first,my_last
        call set_vbc (tile)
      enddo
C$OMP BARRIER


      do tile=my_first,my_last
        call bulk_flux (tile)
      enddo
C$OMP BARRIER

C$OMP BARRIER

      if (may_day_flag.ne.0) return  !-->  EXIT



      do tile=my_first,my_last
        call lmd_vmix (tile)
        call omega (tile)
      enddo
C$OMP BARRIER

      do tile=my_first,my_last
        call prsgrd (tile)
        call rhs3d (tile)
        call pre_step3d (tile)
      enddo
C$OMP BARRIER

      do tile=my_first,my_last
        call uv3dmix_spg (tile)

!---------------------------

        call set_avg (tile)
      enddo

C$OMP BARRIER
C$OMP MASTER


      nrhs=3
      nnew=3-nstp


!
! Output block: write restart/history files.
!
      call output

C$OMP END MASTER
C$OMP BARRIER

      if (may_day_flag.ne.0) return  !-->  EXIT


      return
      end
