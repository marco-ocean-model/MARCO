!

      integer IstrR,IendR,JstrR,JendR
      integer IstrU
      integer JstrV

      if ((istr.eq.1 .and. .not.WEST_INTER)) then
        IstrR=Istr-1
        IstrU=Istr+1
      else
        IstrR=Istr
        IstrU=Istr
      endif

      if ((iend.eq.Lmmpi .and. .not.EAST_INTER)) then
        IendR=Iend+1
      else
        IendR=Iend
      endif

      if ((jstr.eq.1.and. .not.SOUTH_INTER)) then
        JstrR=Jstr-1
        JstrV=Jstr+1
      else
        JstrR=Jstr
        JstrV=Jstr
      endif

      if ((jend.eq.Mmmpi .and. .not.NORTH_INTER)) then
        JendR=Jend+1
      else
        JendR=Jend
      endif
!

!
!----------------------------------------------------------------------
!  Initialization of free surface zeta arrays and wet/dry mask
!
!  During initialization and/or restart: copy initial free surface
!  field into array for holding fast-time averaged free surface.
!----------------------------------------------------------------------
!
      if (iic.eq.0) then
!$acc kernels if(compute_on_device) default(present)
        do j=JstrR,JendR
          do i=IstrR,IendR
            hinv(i,j)=1./(abs(h(i,j))+hc)
            Zt_avg1(i,j)=zeta(i,j,knew)  ! knew=1 unless modified
                                         ! in ana_initial (JET)
          enddo
        enddo

!$acc end kernels
        call exchange_r2d_tile (Istr,Iend,Jstr,Jend, hinv)
      endif   !  iic==0  <-- end initialization
!
!----------------------------------------------------------------------
!  Set vertical grid
!----------------------------------------------------------------------
!
      call exchange_r2d_tile (Istr,Iend,Jstr,Jend, Zt_avg1)
!$acc kernels if(compute_on_device) default(present)

      do j=JstrR,JendR              !!! WARNING: Setting must be consistent
        do i=IstrR,IendR            !!!          with omega.F
          z_w(i,j,0)=-h(i,j)
        enddo

        do k=1,N,+1
          cff_w =hc*sc_w(k)
          cff_r =hc*sc_r(k)
          cff1_w=Cs_w(k)
          cff1_r=Cs_r(k)
          do i=IstrR,IendR
            zetatmp=Zt_avg1(i,j)
            zetatmp=zetatmp*rmask(i,j)
            htmp=abs(h(i,j))
            z_w0=cff_w+cff1_w*htmp
            z_r0=cff_r+cff1_r*htmp
            z_w(i,j,k)=z_w0*h(i,j)*hinv(i,j)+zetatmp
     &                                       *(1.+z_w0*hinv(i,j))
            z_r(i,j,k)=z_r0*h(i,j)*hinv(i,j)+zetatmp
     &                                       *(1.+z_r0*hinv(i,j))
          enddo
        enddo
      enddo

      do k=1,N
        do j=JstrR,JendR
          do i=IstrR,IendR
            Hz_bak(i,j,k)=Hz(i,j,k)
            Hz(i,j,k)=z_w(i,j,k)-z_w(i,j,k-1)

          enddo
        enddo
      enddo

!$acc end kernels


      call exchange_w3d_tile (Istr,Iend,Jstr,Jend, 
     &                        z_w(-1,-1,0))
      call exchange_r3d_tile (Istr,Iend,Jstr,Jend,
     &                        z_r(-1,-1,1))
      call exchange_r3d_tile (Istr,Iend,Jstr,Jend,
     &                        Hz(-1,-1,1))
      call exchange_r3d_tile (Istr,Iend,Jstr,Jend,
     &                        Hz_bak(-1,-1,1))
      return
      end

